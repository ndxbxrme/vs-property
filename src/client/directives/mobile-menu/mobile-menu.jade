.mobile-menu(ng-click='mobileMenuOut=false')
  nav
    ul.menu
      li(ng-class="{selected:isSelected('dashboard')}")
        a(href='/') 
          span Dashboard
      li(ng-class="{selected:propertyPage||isSelected('properties')}")
        a(href='/property') 
          span Property
      li(ng-class="{selected:isSelected('viewings')}")
        a(href='/viewings') 
          span Viewings
      li(ng-class="{selected:isSelected('feedback')}")
        a(href='/feedback') 
          span Feedback
      li(ng-class="{selected:isSelected('offers')}")
        a(href='/offers') 
          span Offers